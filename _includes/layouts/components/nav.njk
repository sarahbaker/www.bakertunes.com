<nav aria-label="Main navigation">
  <div class="main-nav">
    <ul>
    {%- for nav in collections.mainNav -%}
      <li>
        <a href="{{ nav.url | url }}"{% activepath nav.url, page.url %}>{{ nav.data.navtitle }}</a>
      </li>
    {%- endfor -%}
      <li>
        <a href="https://medium.com/bakertunes" target="_blank">Blog</a>
      </li>
    </ul>
  </div>

  <div class="secondary-nav">
    <ul>
      {%- for primary in collections.primaryNav -%}
      <li>
        <a href="{{ primary.url | url }}"{% if primary.url == page.url %} data-current="current item" class="current" aria-current="page" {% endif %}>{{ primary.data.navtitle }}</a>
      </li>
      {%- endfor -%}
    </ul>
  </div>
</nav>